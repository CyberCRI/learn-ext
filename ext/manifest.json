{
  "name": "ilearn Proto",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "ilearn prototype",
  "homepage_url": "https://noop.pw",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "bookmarks",
    "contextMenus",
    "history",
    "idle",
    "tabs",
    "activeTab",
    "https://*/*",
    "http://*/*"
  ],
  "omnibox": {
    "keyword": "ilearn"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "src/*"
  ]
}